Host *.launchpad.net
	User juliank
    ControlPath ~/.ssh/master-%r@bazaar.launchpad.net:22
	StrictHostKeyChecking yes

Host git.debian.org svn.debian.org bzr.debian.org hg.debian.org alioth.debian.org
	ControlPath ~/.ssh/master-debian-alioth

Host ravel.debian.org people.debian.org ssh.upload.debian.org
	ControlPath ~/.ssh/master-debian-ravel

Host *.debian.org *.debian.net
	UserKnownHostsFile ~/.ssh/debian_known_hosts

Host *.mathematik.uni-marburg.de *.informatik.uni-marburg.de
	User klode

Host *.uni-marburg.de
	User Klode

Host *
	ControlMaster auto
	ControlPath ~/.ssh/master-%r@%h:%p
	ServerAliveInterval 300
	SetupTimeOut 300

